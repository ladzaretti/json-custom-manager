{
  "dependencyDashboard": true,
    "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["*image.json$"],
      "matchStrings": ["ghcr.io/renovatebot/renovate:(?<currentValue>.*?)\""],
      "depNameTemplate": "ghcr.io/renovatebot/renovate",
      "datasourceTemplate": "docker"
    }
  ]
}
